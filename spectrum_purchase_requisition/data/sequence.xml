<odoo>
    <data>
        <record id="seq_purchase_requisition_order" model="ir.sequence">
            <field name="name">Purchase Requisition</field>
            <field name="code">purchase.requisition.code</field>
            <field name="prefix">PR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="vendor_sequence" model="ir.sequence">
            <field name="name">Vendor Sequence</field>
            <field name="code">vendor.sequence</field>
            <field name="prefix">VNO</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
         <record id="sale_order_approval_cron" model="ir.cron">
            <field name="name">Scheduled Sale Order Auto-Approval</field>
            <field name="model_id" ref="spectrum_purchase_requisition.model_sale_order"/>
            <field name="state">code</field>
            <field name="code">env['sale.order'].sale_order_auto_approval()</field>
            <field name="interval_type">minutes</field>
            <field name="interval_number">5</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>